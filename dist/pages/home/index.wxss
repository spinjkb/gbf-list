.photo_head {
  width: 100%;
  display: flex;
  flex-flow: row wrap;
  align-content: flex-start;
  padding-left: 2%;
  padding-right: 2%;
}
.photo_head .photo_head_item {
  box-sizing: border-box;
  background-color: white;
  flex: 0 0 46%;
  margin-left: 2%;
  margin-right: 2%;
}
.photo_head .photo_head_item .photo_img {
  background-size: 90% 90%;
  height: 100px;
}
.photo_head .photo_head_item .photo_title {
  text-align: center;
  display: block;
}
.nav_bar {
  background: #39f;
}
.nav_bar .title {
  color: #fff;
  font-size: 16px;
}
.content {
  padding: 10px;
  height: 100px;
}
.content .title {
  font-size: 20px;
  text-align: center;
}
.starContent {
  padding: 50px 50px 0px 50px;
}
.buttonContent {
  padding: 50px;
}
.select_container {
  margin: 10rpx 0;
}
.select_container .ui-row {
  margin-top: 10rpx;
  margin-bottom: 0 !important;
}
.select_container .column-content {
  text-align: center;
  height: 40px;
  line-height: 40px;
  width: 100%;
  background-color: #eee;
  border-radius: 5px;
  position: sticky;
  top: 0rpx;
}
